generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

model Levels {
    memberId String @id
    guildId  String
    level    Int    @default(0)
    xp       Int    @default(0)
}

model Cards {
    memberId   String @id
    guildId    String
    background String @default("https://i.ibb.co/R9dNGQg/d0aed536fc9a360003d5ee26b9555d9f-1.png")
    progress   String @default("white")
}
